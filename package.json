{
  "name": "keen-ui",
  "version": "1.4.0",
  "description": "Keen UI is a Vue.js UI library with a simple API, inspired by Google's Material Design.",
  "main": "dist/keen-ui.js",
  "scripts": {
    "dev": "vite serve --config build/vite.config.js docs-src",
    "build:dist": "webpack --config build/webpack.dist.js",
    "build:docs": "webpack --config build/webpack.docs.js",
    "build:lib": "webpack --config build/webpack.lib.js",
    "build:all": "npm run build:dist && npm run build:lib && npm run build:docs",
    "build:all:prod": "cross-env NODE_ENV=production npm run build:all",
    "clean": "rimraf ./dist ./lib ./docs/docs.bundle.css ./docs/docs.bundle.js",
    "lint": "eslint --ext .js,.vue .",
    "lint:fix": "eslint --fix --ext .js,.vue .",
    "format": "prettier --check \"*.{js,json,md}\" \"{build,src,docs-src}/*.{js,css,scss,vue,html}\"",
    "format:fix": "prettier --write \"*.{js,json,md}\" \"{build,src,docs-src}/*.{js,css,scss,vue,html}\"",
    "test": "npm run lint && npm run format"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosephusPaye/Keen-UI.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "material design",
    "components",
    "ui"
  ],
  "files": [
    "dist",
    "lib",
    "src",
    "CHANGELOG.md",
    "LICENCE",
    "README.md",
    "yarn.lock"
  ],
  "author": {
    "name": "Josephus Paye II"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JosephusPaye/Keen-UI/issues"
  },
  "homepage": "https://github.com/JosephusPaye/Keen-UI/",
  "dependencies": {
    "autosize": "^3.0.20",
    "deepmerge": "^2.0.1",
    "fuzzysearch": "^1.0.3",
    "lodash.debounce": "^4.0.8",
    "tippy.js": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "autoprefixer": "^6.5.4",
    "babel-loader": "^8.2.5",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.8.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "^2.8.1",
    "rimraf": "^2.5.4",
    "sass": "^1.41.0",
    "sass-loader": "^7.3.1",
    "terser-webpack-plugin": "^5.3.5",
    "vite": "^3.0.4",
    "vue": "^3.2.37",
    "vue-loader": "^16.8.3",
    "vue-router": "^4.0.0",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.10.0"
  }
}
